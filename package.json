{
  "name": "eslint-test",
  "version": "1.0.0",
  "description": "Just some files for testing with eslint-teamcity",
  "main": "index.js",
  "scripts": {
    "lint": "node node_modules/.bin/eslint .",
    "lint:teamcity": "node node_modules/.bin/eslint . -f './node_modules/eslint-teamcity/index.js'"
  },
  "author": "Andre Ogle",
  "license": "ISC",
  "eslint-teamcity": {
    "reporter": "errors",
    "report-name": "Andre Report",
    "error-count-name": "How many errors",
    "warning-count-name": "How many warnings"
  },
  "devDependencies": {
    "eslint": "^4.12.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-teamcity": "andreogle/eslint-teamcity#inspections"
  }
}
